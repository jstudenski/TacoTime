<div class="container">


  <h2>TacoTime!</h2>

<form class="add-taco">

  <div class="form-group">
    <label for="name">Name:</label>
    <input type="text" id="name" name="name">
  </div>

  <button type="submit">Add Taco</button>
</form>



  <div class="panel">
    
<h3>Feed me!</h3>


  {{#each tacos}}
    {{#unless eaten}}
       {{> taco-item eaten=false}}
    {{/unless}}
  {{/each}}



  </div>

  <div class="panel">
<h3>Tacos that are eaten!</h3>

  {{#each tacos}}
    {{#if eaten}}

       {{> taco-item eaten=true}}
    {{/if}}
  {{/each}}

  </div>

</div>







